#include <stdio.h>
#include <stdlib.h>

int countSequences(int n) {
    int dp[n + 1];
    dp[0] = 1; // Початковий випадок: 1 спосіб побудувати послідовність довжиною 0 (порожня послідовність).
    dp[1] = 2; // Початковий випадок: 2 способи побудувати послідовність довжиною 1.
    dp[2] = 4; // Початковий випадок: 4 способи побудувати послідовність довжиною 2.

    // Обчислюємо кількість способів побудувати послідовність довжиною n.
    for (int i = 3; i <= n; i++) {
        // За формулою dp[i] = dp[i - 1] + dp[i - 2] + dp[i - 3] обчислюємо кількість способів.
        dp[i] = dp[i - 1] + dp[i - 2] + dp[i - 3];
    }

    return dp[n] % 12345; // Повертаємо кількість способів, обмежену до значення 12345.
}

int main() {
    int n;
    printf("Введіть довжину послідовності n: ");
    scanf("%d", &n);

    int result = countSequences(n);
    printf("Кількість шуканих послідовностей: %d\n", result);

    return 0;
}
