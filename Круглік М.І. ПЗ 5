#include <stdio.h>
#include <stdlib.h>

int countSequences(int n) {
    int dp[n + 1];
    dp[0] = 1;
    dp[1] = 2;
    dp[2] = 4;

    for (int i = 3; i <= n; i++) {
        dp[i] = dp[i - 1] + dp[i - 2] + dp[i - 3];
    }

    return dp[n] % 12345;
}

int main() {
    int n;
    printf("Enter the length of the sequence (n): ");
    scanf("%d", &n);

    int result = countSequences(n);
    printf("Number of valid sequences: %d\n", result);

    return 0;
}
